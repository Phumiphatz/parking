<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <TextView
            android:text="@string/name"
            android:layout_width="282dp"
            android:layout_height="37dp" tools:layout_editor_absoluteY="277dp"
            tools:layout_editor_absoluteX="72dp" android:id="@+id/name"/>
    <TextView
            android:text="@string/plate"
            android:layout_width="282dp"
            android:layout_height="37dp" tools:layout_editor_absoluteY="222dp"
            tools:layout_editor_absoluteX="72dp" android:id="@+id/plate"/>
    <TextView
            android:text="@string/brand"
            android:layout_width="282dp"
            android:layout_height="37dp" tools:layout_editor_absoluteY="333dp"
            tools:layout_editor_absoluteX="72dp" android:id="@+id/brand"/>
    <Button
            android:text="@string/slot1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="106dp"
            tools:layout_editor_absoluteX="41dp" android:id="@+id/slot1"/>
    <Button
            android:text="@string/slot2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="106dp"
            tools:layout_editor_absoluteX="161dp" android:id="@+id/slot2"/>
    <Button
            android:text="@string/slot3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="106dp"
            tools:layout_editor_absoluteX="283dp" android:id="@+id/slot3"/>
    <Button
            android:text="@string/updatebtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="439dp"
            tools:layout_editor_absoluteX="73dp" android:id="@+id/updatebtn"/>
</androidx.constraintlayout.widget.ConstraintLayout>